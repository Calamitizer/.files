{
	"Block Comment": {
		"prefix": "blk",
		"body": [
			"/**",
			" * $1",
			" */",
			"$0"
		],
		"description": "Insert a block comment"
	},

	"Q-Free Copyright": {
		"prefix": "qfc",
		"body": [
			"/**",
			" * @author $1.$2",
			" * Copyright (c) 2018, Open Roads Consulting, Inc.",
			" */",
			"$0"
		],
		"description": "Insert a Q-Free copyright"
	},

	"Parameter Documentation Comment": {
		"prefix": "pdc",
		"body": [
			"/**",
			" * @param $1 $2",
			" */",
			"$0"
		],
		"description": "Insert a parameter documentation comment"
	},

	"Return Documentation Comment": {
		"prefix": "rdc",
		"body": [
			"/**",
			" * @return $1",
			" */",
			"$0"
		],
		"description": "Insert a return documentation comment"
	}
}
