[user]
    name = Alex Ruble
    email = alex.ruble@q-free.com
[core]
    editor = code --wait
    excludesfile = ~/.gitignore-global
[credential "https://github.com"]
    username = calamitizer
[remote "origin"]
    prune = true
[alias]
    psod = push origin OTMS-20405-DSS-Corridor-Control-Schematic
    ########
    a = add
    aa = add --all
    ########
    b = branch
    ########
    cf = config --global -e
    ########
    ch = checkout
    chb = checkout -b
    chm = checkout master
    ########
    c = commit
    cm = commit -m
    ########
    d = diff
    dt = difftool
    ########
    f = fetch
    ########
    h = help
    ########
    l = log
    lom = log origin/master..master
    ########
    ls = ls-files
    ########
    m = merge
    ########
    o = checkout
    ########
    pop = stash pop
    ########
    ps = push
    pso = push origin
    psom = push origin master
    psuo = push -u origin
    ########
    pl = pull
    plo = pull origin
    plom = pull origin master
    plu = pull upstream
    plum = pull upstream master
    ########
    r = remote
    ra = remote add
    rr = remote rm
    rv = remote -v
    ########
    rmc = rm --cached
    ########
    s = status
